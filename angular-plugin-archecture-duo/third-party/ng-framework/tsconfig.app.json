{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "paths": {
      "@duochen/ng-framework-header": ["modules/header/public_api.ts"],
      "@duochen/ng-framework-sidebar": ["modules/sidebar/public_apit.ts"]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "src/test.ts",
    "src/**/*.spec.ts"
  ],
  "angularCompilerOptions": {
    "paths": {
      "@duochen/ng-framework-header": ["modules/header/public_api.ts"],
      "@duochen/ng-framework-sidebar": ["modules/sidebar/public_api.ts"]
    }
  }
}
