{
  "system": {
    "baseURL": "/assets/modules",
    "paths": {
      "npm:": "https://unpkg.com/"
    },
    "map": {
      "@angular/core": "npm:@angular/core@8.2.7/bundles/core.umd.js",
      "@angular/common": "npm:@angular/common@8.2.7/bundles/common.umd.js",
      "@angular/forms": "npm:@angular/forms@8.2.7/bundles/forms.umd.js",
      "@angular/platform-browser": "npm:@angular/platform-browser@8.2.7/bundles/platform-browser.umd.js",

      "rxjs": "npm:rxjs@6.5.3/bundles/rxjs.umd.js",
      "rxjs/operators": "npm:rxjs@6.5.3/operators/index.js",

      "plugins-core": "/assets/modules/plugins-core/bundle.js",
      "plugins-example": "/assets/modules/plugins-example/bundle.js"
    }
  },
  "sidebar": {
    "components": [
      "app-page2",
      "app-page1"
    ]
  },
  "plugins": {
    "modules": {
      "plugins-core": "/assets/modules/plugins-core/bundle.js" ,
      "plugins-example": "/assets/modules/plugins-example/bundle.js"
    },
    "routes": [
      {
        "name": "Plugin 1 (label)",
        "path": "plugin1",
        "component": {
          "module": "plugins-example",
          "componentType": "my-label"
        }
      },
      {
        "name": "Plugin 2 (button)",
        "path": "plugin2",
        "component": {
          "module": "plugins-example",
          "componentType": "my-button"
        }
      },
      {
        "name": "Plugin 3 (form)",
        "path": "plugin3",
        "component": {
          "module": "plugins-example",
          "componentType": "my-form"
        }
      }
    ]
  }
}
